{
  "manifest_version": 3,
  "name": "Scrape and Conquer",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Scrape and Conquer"
  }
}
